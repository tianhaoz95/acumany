{
  "hosting": {
    "public": "build",
    "headers": [{
         "source": "**/*.@(jpg|jpeg|gif|png|js|css)",
         "headers": [{
            "key": "Cache-Control",
            "value": "must-revalidate"
         }]
      }],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
